  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "CI=true react-scripts test --coverage --no-cache",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -o .storybook/build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx,ts,tsx}",
      "!src/app/**/*.stories.{js,jsx,ts,tsx}",
      "!src/app/**/*.style.{js,jsx,ts,tsx}",
      "!src/app/**/*.test.{js,jsx,ts,tsx}",
      "!src/app/config.{js,jsx,ts,tsx}",
      "!src/app/routes.{js,jsx,ts,tsx}"
    ]
  },
  "dependencies": {
